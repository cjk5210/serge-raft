<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="">
	<!-- RSF对外提供服务的IP -->
    <ip>127.0.0.1</ip>
    <!-- 客户端连接服务端的端口 -->
    <clientPort>6001</clientPort>
    <!-- 工作状态，默认:Follower -->
    <state>Follower</state>
    <!-- 服务器ID -->
    <id>server_1</id>
    <!-- 集群服务器列表，第一个端口用于选举，第二个端口用于交换数据 -->
    <servers>
        server_1=127.0.0.1:7001:8001,
        server_2=127.0.0.1:7002:8002,
        server_3=127.0.0.1:7003:8003,
        server_4=127.0.0.1:7004:8004,
        server_5=127.0.0.1:7005:8005,
        server_6=127.0.0.1:7006:8006,
    </servers>
</config>
